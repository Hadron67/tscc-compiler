{"code":"import { JsccError as E } from '../util/E';\r\nimport { Action } from '../grammar/item-set';\r\nfunction testParse(g, pt, tokens) {\r\n    var tk = [];\r\n    for (var i = 0; i < tokens.length; i++) {\r\n        var tid = g.findToken(tokens[i]);\r\n        if (tid === -1) {\r\n            throw new E('cannot recognize \"' + tokens[i] + '\" as a token');\r\n        }\r\n        tk.push(tid);\r\n    }\r\n    var state = [0];\r\n    var stack = [];\r\n    var ret = [];\r\n    function s() {\r\n        return state[state.length - 1];\r\n    }\r\n    function shift(ns) {\r\n        state.push(ns);\r\n        stack.push(g.tokens[tk.shift()].sym);\r\n    }\r\n    function reduce(rule) {\r\n    }\r\n    function dump() {\r\n        var ret = '';\r\n        for (var i = 0; i < stack.length; i++) {\r\n            ret += stack[i] + ' ';\r\n        }\r\n        ret += '| ';\r\n        for (var i = 0; i < tk.length; i++) {\r\n            ret += g.tokens[tk[i]].sym + ' ';\r\n        }\r\n        return ret;\r\n    }\r\n    ret.push(dump());\r\n    do {\r\n        var item = pt.lookupShift(s(), tk[0] || 0);\r\n        if (item !== null) {\r\n            if (item.actionType === Action.SHIFT) {\r\n                shift(item.shift.stateIndex);\r\n            }\r\n            else if (item.actionType === Action.REDUCE) {\r\n                var rule = item.rule;\r\n                var rlen = rule.rhs.length;\r\n                while (rlen-- > 0) {\r\n                    state.pop();\r\n                    stack.pop();\r\n                }\r\n                stack.push(g.nts[rule.lhs].sym);\r\n                if (item.rule.index === 0) {\r\n                    ret.push('accepted!');\r\n                    break;\r\n                }\r\n                else {\r\n                    var gotot = pt.lookupGoto(s(), rule.lhs).shift.stateIndex;\r\n                    state.push(gotot);\r\n                }\r\n            }\r\n            else {\r\n                console.assert(false);\r\n            }\r\n        }\r\n        else {\r\n            ret.push('syntax error!');\r\n            break;\r\n        }\r\n        ret.push(dump());\r\n    } while (true);\r\n    return ret;\r\n}\r\nexport { testParse };\r\n//# sourceMappingURL=parse-test.js.map","map":{"version":3,"file":"parse-test.js","sourceRoot":"","sources":["../src/top/parse-test.ts"],"names":[],"mappings":"AAAA,OAAO,EAAE,SAAS,IAAI,CAAC,EAAE,MAAM,WAAW,CAAC;AAC3C,OAAO,EAAE,MAAM,EAAE,MAAM,qBAAqB,CAAC;AAI7C,mBAAmB,CAAU,EAAE,EAAc,EAAE,MAAgB;IAC3D,IAAI,EAAE,GAAG,EAAE,CAAC;IACZ,GAAG,CAAA,CAAC,IAAI,CAAC,GAAG,CAAC,EAAE,CAAC,GAAG,MAAM,CAAC,MAAM,EAAC,CAAC,EAAE,EAAC,CAAC;QAClC,IAAI,GAAG,GAAG,CAAC,CAAC,SAAS,CAAC,MAAM,CAAC,CAAC,CAAC,CAAC,CAAC;QACjC,EAAE,CAAA,CAAC,GAAG,KAAK,CAAC,CAAC,CAAC,CAAA,CAAC;YACX,MAAM,IAAI,CAAC,CAAC,oBAAoB,GAAG,MAAM,CAAC,CAAC,CAAC,GAAG,cAAc,CAAC,CAAC;QACnE,CAAC;QACD,EAAE,CAAC,IAAI,CAAC,GAAG,CAAC,CAAC;IACjB,CAAC;IACD,IAAI,KAAK,GAAG,CAAE,CAAC,CAAE,CAAC;IAClB,IAAI,KAAK,GAAG,EAAE,CAAC;IACf,IAAI,GAAG,GAAG,EAAE,CAAC;IACb;QACI,MAAM,CAAC,KAAK,CAAC,KAAK,CAAC,MAAM,GAAG,CAAC,CAAC,CAAC;IACnC,CAAC;IACD,eAAe,EAAE;QACb,KAAK,CAAC,IAAI,CAAC,EAAE,CAAC,CAAC;QACf,KAAK,CAAC,IAAI,CAAC,CAAC,CAAC,MAAM,CAAC,EAAE,CAAC,KAAK,EAAE,CAAC,CAAC,GAAG,CAAC,CAAC;IACzC,CAAC;IACD,gBAAgB,IAAI;IAEpB,CAAC;IACD;QACI,IAAI,GAAG,GAAG,EAAE,CAAC;QACb,GAAG,CAAA,CAAC,IAAI,CAAC,GAAG,CAAC,EAAC,CAAC,GAAG,KAAK,CAAC,MAAM,EAAC,CAAC,EAAE,EAAC,CAAC;YAChC,GAAG,IAAI,KAAK,CAAC,CAAC,CAAC,GAAG,GAAG,CAAC;QAC1B,CAAC;QACD,GAAG,IAAI,IAAI,CAAC;QACZ,GAAG,CAAA,CAAC,IAAI,CAAC,GAAG,CAAC,EAAC,CAAC,GAAG,EAAE,CAAC,MAAM,EAAC,CAAC,EAAE,EAAC,CAAC;YAC7B,GAAG,IAAI,CAAC,CAAC,MAAM,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,GAAG,GAAG,GAAG,CAAC;QACrC,CAAC;QACD,MAAM,CAAC,GAAG,CAAC;IACf,CAAC;IACD,GAAG,CAAC,IAAI,CAAC,IAAI,EAAE,CAAC,CAAC;IACjB,GAAE,CAAC;QACC,IAAI,IAAI,GAAG,EAAE,CAAC,WAAW,CAAC,CAAC,EAAE,EAAC,EAAE,CAAC,CAAC,CAAC,IAAI,CAAC,CAAC,CAAC;QAC1C,EAAE,CAAA,CAAC,IAAI,KAAK,IAAI,CAAC,CAAA,CAAC;YACd,EAAE,CAAA,CAAC,IAAI,CAAC,UAAU,KAAK,MAAM,CAAC,KAAK,CAAC,CAAA,CAAC;gB