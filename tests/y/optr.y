%token 
'+' '-' '*' '/' '**' '?' ':' '%'
'>' '<' '>=' '<=' '==' '!='
'=' '+=' '-=' '*=' '/=' '&=' '|=' '^=' '>>=' '<<=' '%='
'|' '&' '^' '~'
'&&' '||' '!'
'>>' '<<' '>>>'

%right '=' '+=' '-=' '*=' '/=' '&=' '|=' '^=' '>>=' '<<=' '%='
%left '?' ':'
%left '||'
%left '&&'
%right '!'
%left '|'
%left '^'
%left '&'
%right '~'
%left '>' '<' '>=' '<=' '==' '!='
%left '>>' '<<' '>>>'
%left '+' '-'
%left '*' '/' '%'
%right '**'

%token 'id' 'num'

%%



e: 
  ae aoptr e %prec '='
| e '?' e ':' e
| e '||' e
| e '&&' e
| e '|' e
| e '^' e
| e '&' e
| e coptr e %prec '>'
| e '>>' e | e '<<' e | e '>>>' e
| e '+' e | e '-' e
| e '*' e | e '/' e | e '%' e
| e '**' e
| '~' e
| '!' e
| 'id'
| 'num'
;

ae: 'id';
aoptr: '=' | '+=' | '-=' | '*=' | '/=' | '&=' | '|=' | '^=' | '>>=' | '<<=' | '%=' ;
coptr: '>' | '<' | '>=' | '<=' | '==' | '!=' ;

%%